{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter (Local API)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--dart-define=API_BASE_URL=http://localhost:8000/api"
